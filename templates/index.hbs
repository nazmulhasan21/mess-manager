<html lang='en'>
  <head>
    <meta charset='UTF-8' />
    <meta http-equiv='X-UA-Compatible' content='IE=edge' />
    <meta name='viewport' content='width=device-width, initial-scale=1.0' />
    <title>{{title}}</title>
    <link
      href='https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css'
      rel='stylesheet'
      integrity='sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC'
      crossorigin='anonymous'
    />

  </head>
  <body>
    <div class=''>
      <h1 class='text-center'>{{messName}}</h1>
      <h5 class='text-center'>Manager Name:{{name}}</h5>

      <h6 class='text-center'>Month Title:{{monthTitel}}</h6>
      <h6 class='text-center'>Rich Balance:{{richBalance}}</h6>

      <table class='table'>
        <thead>
          <tr>
            <th>Name</th>
            <th>meal</th>
            <th>mealCost</th>
            <th>OtherCost</th>
            <th>totalCost</th>
            <th>Deposit</th>
            <th>balance</th>
            <th>richBalance</th>
          </tr>
        </thead>
        <tbody>
          {{#each allMember as |member|}}
            <tr>
              <td>{{member.name}}</td>
              <td>{{member.totalMeal}}</td>
              <td>{{member.mealCost}}</td>
              <td>{{member.otherCost}}</td>
              <td>{{member.totalCost}}</td>
              <td>{{member.totalDeposit}}</td>
              <td>{{member.balance}}</td>
              <td>{{member.richBalance}}</td>
            </tr>
          {{/each}}

        </tbody>
      </table>

